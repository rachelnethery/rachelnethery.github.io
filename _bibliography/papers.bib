	keywords = {MoBa, The Norwegian Mother, attention-deficit hyperactivity disorder, Father and Child Cohort Study, hyperthyroidism, hypothyroidism, hypothyroxinemia, maternal, thyroid},
	pages = {218--228},
	file = {Snapshot:/Users/ran601/Zotero/storage/C29MVH5X/ppe.html:text/html},
}

