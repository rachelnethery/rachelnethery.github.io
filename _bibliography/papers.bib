	urldate = {2023-05-01},
	journal = {Science of The Total Environment},
	author = {Yitshak-Sade, Maayan and Nethery, Rachel and Schwartz, Joel D. and Mealli, Fabrizia and Dominici, Francesca and Di, Qian and Abu Awad, Yara and Ifergane, Gal and Zanobetti, Antonella},
	month = feb,
	year = {2021},
	keywords = {PM, Dementia, Parkinson's disease, Particulate air pollution},
	pages = {142524},
	file = {ScienceDirect Full Text PDF:/Users/ran601/Zotero/storage/3ZDRCS5H/Yitshak-Sade et al. - 2021 - PM2.5 and hospital admissions among Medicare enrol.pdf:application/pdf;ScienceDirect Snapshot:/Users/ran601/Zotero/storage/FW2HTX3P/S0048969720360538.html:text/html},
}

