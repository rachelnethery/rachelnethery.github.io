	url = {https://www.sciencedirect.com/science/article/pii/S1877584525000279},
	doi = {10.1016/j.sste.2025.100736},
	abstract = {Syndromic surveillance monitors infectious diseases, especially in situations where direct disease monitoring is unavailable. However, conventional syndromic surveillance methods face challenges in handling missing data, particularly when the missing completely at random (MCAR) assumption is violated. Additionally, these methods often do not leverage spatio-temporal techniques that can reduce bias and improve their performance. This study addresses both of these limitations by comparing a baseline syndromic surveillance model with a frequentist spatio-temporal model used in infectious diseases and a Bayesian spatio-temporal conditional autoregressive (CAR) model. Drawing inspiration from COVID-19 symptom data collected via routine health systems in Liberia, we conduct simulations with various data generating processes, spatio-temporal correlation structures, and missing data mechanisms. Across the diverse simulations for outbreak detection, the baseline model and the Bayesian CAR model had high specificity, thus limiting outbreak false alarms. The findings underscore the importance of considering spatio-temporal models for syndromic surveillance.},
	urldate = {2025-08-06},
	journal = {Spatial and Spatio-temporal Epidemiology},
	author = {Link, Nicholas B. and Gopaluni, Anuraag and Fulcher, Isabel and Boley, Emma Jean and Nethery, Rachel C. and Hedt-Gauthier, Bethany},
	month = aug,
	year = {2025},
	keywords = {Conditional autoregressive models, Routine health data, Spatial methods, Syndromic surveillance, Temporal methods},
	pages = {100736},
	file = {Link et al. - 2025 - Spatio-temporal methods to handle missing data in .pdf:/Users/ran601/Zotero/storage/37GKGCB5/Link et al. - 2025 - Spatio-temporal methods to handle missing data in .pdf:application/pdf;ScienceDirect Snapshot:/Users/ran601/Zotero/storage/UFHFMAQ3/S1877584525000279.html:text/html},
