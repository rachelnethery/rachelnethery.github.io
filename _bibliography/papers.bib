	number = {7},
	urldate = {2023-05-01},
	journal = {BJOG: An International Journal of Obstetrics \& Gynaecology},
	author = {Grace, Mr and Vladutiu, Cj and Nethery, Rc and Siega-Riz, Am and Manuck, Ta and Herring, Ah and Savitz, D and Thorp, Jt},
	year = {2018},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/1471-0528.14927},
	keywords = {preterm birth, Cholesterol, dyslipidaemia, gestational age at delivery, lipoproteins, nuclear magnetic resonance spectroscopy},
