	journal = {Environmental Research},
	author = {Chen, Naiyu and Hu, Cindy R. and Iyer, Hari S. and James, Peter and Dickerman, Barbra A. and Mucci, Lorelei A. and Nethery, Rachel C.},
	month = dec,
	year = {2024},
	keywords = {Green space, Quality of life, Cancer survivorship, Neighborhood greenness, Prostate cancer},
	pages = {119847},
